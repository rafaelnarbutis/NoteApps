<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.senac.noteapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.InfoNoteActivity"
            android:parentActivityName=".activities.ListNotesActivity"></activity>
        <activity
            android:name=".activities.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName=".activities.ListNotesActivity" />
        <activity
            android:name=".activities.UserActivity"
            android:parentActivityName=".activities.ListNotesActivity" />
        <activity
            android:name=".activities.NewNoteActivity"
            android:label="Criar nota"
            android:parentActivityName=".activities.ListNotesActivity" />
        <activity
            android:name=".activities.ListNotesActivity"
            android:label="Todas as notas">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>